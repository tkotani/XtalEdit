# SpaceGroupNo= 221  Choice= 1  ! generated by TSPACE (Dr.A.Yanase).
Bohr_R	= 0.529177249  # [BohrRadius/Angstrom]
PI = 3.14159265358979323846
# a, as a scale of system, is a required key word!
a =   3.8348/Bohr_R           # give in unit of Bohr_R(=atomic unit).
CV1 = a * EX
CV2 = a * EY
CV3 = a * EZ

PV1o =  CV1
PV2o =  CV2
PV3o =  CV3


SV1n =   + PV1o + PV2o - PV3o
SV2n =   - PV1o + PV2o + PV3o
SV3n =   + PV1o - PV2o + PV3o
PV1 = SV1n
PV2 = SV2n
PV3 = SV3n

Trans0 = 0.
Trans1 =  + PV3o
Trans2 =  + PV2o
Trans3 =  + PV1o


# SITE TYPE=    LaSr, POS=0.               #  1a
